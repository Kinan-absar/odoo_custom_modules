<odoo>
    <!-- Add vendor menu items to the existing left sidebar -->
    <template id="vendor_sidebar_menu" inherit_id="portal.portal_sidebar">
        <xpath expr="//ul" position="inside">
            <t t-if="request.env.user.partner_id.supplier_rank &gt; 0">

                <li>
                    <a href="/vendor">
                        <span t-translate="yes">Vendor Home</span>
                    </a>
                </li>

                <li>
                    <a href="/vendor/pos">
                        <span t-translate="yes">My Purchase Orders</span>
                    </a>
                </li>

                <li>
                    <a href="/vendor/invoices">
                        <span t-translate="yes">My Invoices</span>
                    </a>
                </li>

                <li>
                    <a href="/vendor/invoice/upload">
                        <span t-translate="yes">Upload Invoice</span>
                    </a>
                </li>

            </t>
        </xpath>
    </template>
</odoo>
