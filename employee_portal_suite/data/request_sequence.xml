<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_employee_request" model="ir.sequence">
            <field name="name">Employee Request Sequence</field>
            <field name="code">employee.request.seq</field>
            <field name="prefix">ERQ/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_material_request" model="ir.sequence">
            <field name="name">Material Request Sequence</field>
            <field name="code">material.request.seq</field>
            <field name="prefix">MR-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
